<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.dao.BaiZhiOrderDAO">
	<insert id="insert" parameterType="BaiZhiOrder">
		insert into baizhi_order(id,userid,type,demand,number,price,description) values(#{id},#{userid},#{type},#{demand},#{number},#{price},#{description})
	</insert>
</mapper>


